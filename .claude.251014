alPriceVolume = math.sum(typicalPriceVolume, period)\n    cumulativeVolume = math.sum(volume, period)\n    cumulativeTypicalPriceVolume / cumulativeVolume\n\n// ë¼ì¸ ê³„ì‚°\nMAP1 = calcLine(MAP1Periods)\nMAP2 = calcLine(MAP2Periods)\nMAP3 = calcLine(MAP3Periods)\nMAP4 = calcLine(MAP4Periods)\n\n\nplot(ShowMAP1?MAP1:na, color=MAP1Color,  linewidth=1, title=\"MAP1 Line\")\nplot(ShowMAP2?MAP2:na, color=MAP2Color,  linewidth=1, title=\"MAP2 Line\")\nplot(ShowMAP3?MAP3:na, color=MAP3Color,  linewidth=2, title=\"MAP3 Line\")\nplot(ShowMAP4?MAP4:na, color=MAP4Color,  linewidth=3, title=\"MAP4 Line\")\n\n\n// Function to generate future lines with a specific calculation function\ngenerate_future_lines(period, startOffset, lineColor, lineWidth, lineStyle, calcType) =>\n    futureArray = array.new_float(period, 0.0)\n    var float value = na  // ëª…ì‹œì ìœ¼ë¡œ íƒ€ì… ì„ ì–¸\n    for i = 0 to period - 1\n        if calcType == \"MAP Future\"\n            value := calcLine(period - i)\n        array.set(futureArray, i, value)\n\n    if barstate.islast\n        for i = 0 to 10\n            current = i\n            next = i + 1\n            currentValue = array.get(futureArray, current)\n            nextValue = array.get(futureArray, next)\n            if not na(currentValue) and not na(nextValue)\n                line.new(bar_index + startOffset + current, currentValue, \n                         bar_index + startOffset + next, nextValue, \n                         width=lineWidth, color=lineColor, style=lineStyle)\n\n\n// Generate Baseline future\ngenerate_future_lines(MAP1Periods, 0, MAP1Color, 2, line.style_dashed, \"MAP Future\")\ngenerate_future_lines(MAP2Periods, 0, MAP2Color, 2, line.style_dashed, \"MAP Future\")\ngenerate_future_lines(MAP3Periods, 0, MAP3Color, 2, line.style_dashed, \"MAP Future\")\ngenerate_future_lines(MAP4Periods, 0, MAP4Color, 2, line.style_dashed, \"MAP Future\")"
